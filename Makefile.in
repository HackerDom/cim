all:libcim.a cim.ps info

simenvir.o: simenvir.c
	$(RCC) $(ALL_RCFLAGS) -c $(srcdir)/simenvir.c

simfile.o: simfile.c cim.h config.h
	$(RCC) $(ALL_RCFLAGS) -c $(srcdir)/simfile.c

libcim.a: $(ROFILES) $(LIBDEPENDDATAFILES)
	if test -r ./simenvir.atr; then :; else \
          if ln $(srcdir)/simenvir.atr .; then :; else \
            cp -p $(srcdir)/simenvir.atr .; fi; fi
	if test -r ./simfile.atr; then :; else \
          if ln $(srcdir)/simfile.atr .; then :; else \
            cp -p $(srcdir)/simfile.atr .; fi; fi
	if test -r ./simset.sim; then :; else \
          if ln $(srcdir)/simset.sim .; then :; else \
            cp -p $(srcdir)/simset.sim .; fi; fi
	if test -r ./simulation.sim; then :; else \
          if ln $(srcdir)/simulation.sim .; then :; else \
            cp -p $(srcdir)/simulation.sim .; fi; fi
	$(arcommandline)
	- if test "$(ranlib)" = ":" ; then :; else \
            $(ranlib) libcim.a; fi;

