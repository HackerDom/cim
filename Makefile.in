install: install-bin install-include install-lib install-man install-doc install-info

install-bin: installdirs cim
	if $(INSTALL_PROGRAM) cim $(bindir)/`echo cim|sed '$(transform)'`; \
        then :; else \
          $(INSTALL_PROGRAM) cim.exe $(bindir)/`echo cim|sed '$(transform)'`.exe; fi
	- $(INSTALL_PROGRAM) cim2latex $(bindir)/`echo cim2latex|sed '$(transform)'`
	- $(INSTALL_PROGRAM) cim2ps $(bindir)/`echo cim2ps|sed '$(transform)'`
	- $(INSTALL_PROGRAM) cimindent $(bindir)/`echo simindent|sed '$(transform)'`
	- $(INSTALL_PROGRAM) cimetags $(bindir)/`echo cimetags|sed '$(transform)'`

install-lib: installdirs libcim.a uninstalloldlib
	$(INSTALL_DATA) libcim.a $(libdir)/libcim.a
	- if test "$(ranlib)" = ":" ; then :; else \
            $(ranlib) $(libdir)/libcim.a; fi;
	if test "" = "$(LIBDEPENDDATAFILES)"; then \
          $(INSTALL_DATA) $(srcdir)/libcim.atr $(libdir)/libcim.atr; else :; fi

install-include: installdirs cim.h
	$(INSTALL_DATA) $(srcdir)/cim.h $(includedir)/cim.h

install-man: installdirs cim.1
	- $(INSTALL_DATA) $(srcdir)/cim.1 $(mandir)/cim.$(manext)

install-doc: installdirs cim.ps
	- $(INSTALL_DATA) $(srcdir)/cim.ps $(docdir)/cim.ps

install-info: installdirs info
	- $(INSTALL_DATA) $(srcdir)/cim.info $(infodir)/cim.info

installdirs:
	- $(srcdir)/mkinstalldirs $(includedir) $(bindir) $(mandir) $(docdir) $(libdir) $(infodir)

chowninstall:
	chown $(INSTALL_USER) $(bindir)/`echo cim|sed '$(transform)'`
	chown $(INSTALL_USER) $(bindir)/`echo cim2latex|sed '$(transform)'`
	chown $(INSTALL_USER) $(bindir)/`echo cim2ps|sed '$(transform)'`
	chown $(INSTALL_USER) $(bindir)/`echo cimindent|sed '$(transform)'`
	chown $(INSTALL_USER) $(bindir)/`echo cimetags|sed '$(transform)'`
	chown $(INSTALL_USER) $(libdir)/libcim.a
	if test "" = "$(LIBDEPENDDATAFILES)" ; then \
          chown $(INSTALL_USER) $(libdir)/libcim.atr; else :; fi
	chown $(INSTALL_USER) $(includedir)/cim.h
	- chown $(INSTALL_USER) $(mandir)/cim.$(manext)
	- chown $(INSTALL_USER) $(docdir)/cim.ps
	- chown $(INSTALL_USER) $(infodir)/cim.info

uninstall:
	cd $(bindir); rm -f `echo cim|sed '$(transform)'`
	cd $(bindir); rm -f `echo cim2latex|sed '$(transform)'`
	cd $(bindir); rm -f `echo cim2ps|sed '$(transform)'`
	cd $(bindir); rm -f `echo cimindent|sed '$(transform)'`
	cd $(bindir); rm -f `echo cimetags|sed '$(transform)'`
	cd $(libdir); rm -f libcim.a
	if test "" = "$(LIBDEPENDDATAFILES)" ; then \
          cd $(libdir); rm -f libcim.atr; else :; fi
	cd $(includedir); rm -f cim.h
	cd $(mandir); rm -f cim.$(manext)
	cd $(docdir); rm -f cim.ps
	cd $(infodir) rm -f cim.info

uninstalloldlib:
	cd $(oldlibdir); rm -f libcim.a

check:
	./cim $(CIMFLAGS) $(srcdir)/test -r

installcheck:
	rm -rf tmptest
	mkdir tmptest;
	if ln $(srcdir)/test.sim tmptest/.; then :; else \
          cp -p $(srcdir)/test.sim tmptest/.; fi
	cd tmptest; $(bindir)/cim test -r
	rm -rf tmptest

Makefile: Makefile.in config.status
	$(SHELL) config.status

config.h: stamp-h
stamp-h: config.h.in config.status
	$(SHELL) config.status

config.status: $(srcdir)/configure version.h
	$(SHELL) config.status --recheck

$(srcdir)/configure: configure.in
	cd $(srcdir); autoconf

mostlyclean:
	rm -f cim simset.atr simset.c simset.h
	rm -f simulation.atr simulation.c simulation.h
	rm -f *.o core

clean: mostlyclean
	rm -f libcim.a

distclean: clean
	rm -f Makefile config.status config.h

realclean: distclean
	rm -f y.tab.c y.tab.h
	rm -f TAGS cim.dvi cim.ps libcim.atr

TAGS:
	etags c*.c r*.c c*.h dad.h limit.h Makefile.in configure.in \
          config.h.in cparser.y

$(srcdir)/cim.ps: $(srcdir)/cim.dvi
	cd $(srcdir); dvips cim.dvi -o cim.ps

info: $(srcdir)/cim.info 

$(srcdir)/cim.info: cim.texi
	cd $(srcdir); $(MAKEINFO) cim.texi --output=cim.info

dvi: $(srcdir)/cim.dvi

$(srcdir)/cim.dvi: cim.texi
	cd $(srcdir); $(TEXI2DVI) cim.texi

libcim.atr: $(LIBDEPENDDATAFILES)
	if test -r simset.sim; then :; else \
          if ln $(srcdir)/simset.sim .; then :; else \
            cp -p $(srcdir)/simset.sim .; fi; fi
	if test -r simulation.sim; then :; else \
          if ln $(srcdir)/simulation.sim .; then :; else \
            cp -p $(srcdir)/simulation.sim .; fi; fi
	$(ar) libcim.atr $(LIBDEPENDDATAFILES)

dist: Makefile $(DISTFILES)
	rm -rf $(SIMID)
	mkdir $(SIMID)
	for file in $(DISTFILES); do \
          if ln $$file $(SIMID); then :; else \
            cp -p $$file $(SIMID); fi; \
        done
	chmod ugo=rwx $(SIMID)
	chmod ugo+r $(SIMID)/*
	$(tar) $(SIMID).tar $(SIMID)
	$(compress) $(SIMID).tar
	rm -rf $(SIMID)

ftp: dist
	mv $(SIMID).tar.gz ../ftp/.
	chmod a=r ../ftp/$(SIMID).tar.gz
	zcat ../ftp/$(OLDID).tar.gz |tar xf -
	zcat ../ftp/$(SIMID).tar.gz |tar xf -
	- diff $(OLDID) $(SIMID) >../ftp/$(OLDID)-$(SIMID).diff
	rm -fr $(OLDID) $(SIMID)
	$(compress) ../ftp/$(OLDID)-$(SIMID).diff
	chmod a=r ../ftp/$(OLDID)-$(SIMID).diff.gz

# Prevent GNU make v3 from overflowing arg limit on SysV.
.NOEXPORT:
