SHELL = /bin/sh

OLDID = @oldid@
SIMID = @simid@

srcdir = @srcdir@
VPATH = @srcdir@

# Variables controlling the installation.
prefix = @prefix@
exec_prefix = @exec_prefix@
transform = @program_transform_name@

# Directory in which to install executables.
bindir = $(exec_prefix)/bin

# Directory in which to install library files.
libdir = $(exec_prefix)/lib/cim
oldlibdir = $(exec_prefix)/lib

# Directory in which to install man pages.
mandir = $(prefix)/man/man1
manext = 1

# Directory in which to install include file.
includedir = $(prefix)/include

# Directory in which to install formated documentation.
docdir = $(prefix)/doc

# Directory in which to install documentation info files.
infodir = $(prefix)/info

INSTALL = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
INSTALL_DATA = @INSTALL_DATA@
INSTALL_USER = @USER@
MAKEINFO = makeinfo
TEXI2DVI = texi2dvi

# Variables controlling compilation.
CPPFLAGS = @CPPFLAGS@
LIBS = @LIBS@
CFLAGS = @CFLAGS@
LDFLAGS = @LDFLAGS@

# Variables controlling compilation of the generated code.
SCPPFLAGS = $(CPPFLAGS)
SCC = $(CC)
SCFLAGS = $(CFLAGS)
SLDFLAGS = $(LDFLAGS) -o
SLIBS = $(LIBS)

# Variables controlling compilation of the compiler program.
CCPPFLAGS = $(CPPFLAGS)
CCC = $(CC)
CCFLAGS = $(CFLAGS)
ALL_CCFLAGS = -I. -I$(srcdir) '-DINCLUDEDIR="$(includedir)"' '-DLIBDIR="$(libdir)"' $(CCFLAGS) '-DSCC="$(SCC)"' '-DSCFLAGS="$(SCFLAGS) -c"' '-DSLIBS="$(SLIBS)"' '-DSLDFLAGS="$(SLDFLAGS)"'
CLDFLAGS = $(LDFLAGS)
CLIBS = $(LIBS)
CPARFLAGS = 
yacc = @YACC@

# Variables controlling compilation of the run-time system.
RCPPFLAGS = $(CPPFLAGS)
RCC = $(CC)
RCFLAGS = $(CFLAGS)
ALL_RCFLAGS = -I. -I$(srcdir) $(RCFLAGS)
RLD = @RLD@
RLD_O = @RLD_O@

# Variable controlling the Compilation of class Simset and class Simulation.
CIMFLAGS = -h -L -L. -I$(srcdir) -d

ranlib = @RANLIB@
ln = @LN_S@
ar = ar rcuv
tar = tar cf
compress = gzip

arcommandline= @AR_COMMAND_LINE@
longarline= $(ar) libcim.a $(LIBDEPENDDATAFILES) $(ROFILES)
shortarline= $(ar) libcim.a $(LIBDEPENDDATAFILES); \
        for file in $(ROFILES); do \
          $(ar) libcim.a $$file; \
        done

LIBDATAFILES = simulation.sim simset.sim \
simenvir.atr simfile.atr simset.atr simulation.atr

SPLIT = 
NOSPLIT = $(LIBDATAFILES)
LIBDEPENDDATAFILES = @SPLIT@

